<%- include("partials/header"); -%>

<div class="container form-container">
    <form action="/" method="post">
  <div class="form-row">
    <div class="form-group col-md-6">
      <label>First Name</label>
      <input type="text" class="form-control" name="fName" required >
    </div>
    <div class="form-group col-md-6">
      <label>Last Name</label>
      <input type="text" class="form-control" name="lName" required>
    </div>
  </div>
        
    <div class="form-row">
    <div class="form-group col-md-6">
      <label>Admission Number</label>
      <input type="text" class="form-control"  name="aNum" required >
    </div>
    <div class="form-group col-md-6">
      <label>Phone Number 1</label>
      <input type="text" class="form-control" name="pNo" required>
    </div>
  </div>
    
     <div class="form-row">
    <div class="form-group col-md-6">
      <label>Aadhar Number</label>
      <input type="text" class="form-control" name="aadhar" required >
    </div>
    <div class="form-group col-md-6">
      <label>Email-Address</label>
      <input type="email" class="form-control" name="email" required>
    </div>
  </div>
        
     <div class="form-row">
    <div class="form-group col-md-6">
      <label>Workplace/College Name</label>
      <input type="text" class="form-control" name="wName" required>
    </div>
    <div class="form-group col-md-6">
      <label>Home Town</label>
      <input type="text" class="form-control" name="hAddr" required>
    </div>
  </div>
    
     <div class="form-row">
    <div class="form-group col-md-6">
      <label>Rent</label>
      <input type="text" class="form-control" name="rent" required>
    </div>
    <div class="form-group col-md-6">
      <label>Advance</label>
      <input type="text" class="form-control" name="advance" required>
    </div>
  </div>
        
  <button type="submit" class="btn btn-primary">Add</button>
</form>
</div>



<%- include("partials/footer"); -%>